{
  "name": "marble-js",
  "version": "0.1.0",
  "description": "Reactive Node APIs made easy",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "npm run watch",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "example": "ts-node example/index.ts",
    "example:watch": "nodemon --watch src --watch example --ext ts --ignore '*.spec.ts' --exec npm run example",
    "watch": "tsc -w",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "test": "NODE_ENV=test jest --expand --coverage",
    "test:watch": "NODE_ENV=test jest --expand --onlyChanged --watch",
    "prepush": "npm run lint",
    "prepublishOnly": "npm run build",
    "doc": "typedoc --out doc src --name 'node-rx doc' --target ES6 --exclude \"**/*+(index|env|.spec|.e2e).ts\" --hideGenerator --mode modules --readme none"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JozefFlakus/marble-js.git"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "keywords": [
    "API",
    "JavaScript",
    "RxJs",
    "TypeScript"
  ],
  "authors": [
    "JÃ³zef Flakus"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JozefFlakus/marble-js/issues"
  },
  "homepage": "https://github.com/JozefFlakus/marble-js#readme",
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/path-to-regexp": "^1.7.0",
    "chalk": "^2.4.1",
    "path-to-regexp": "^2.2.1",
    "rxjs": "~6.1.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/supertest": "~2.0.4",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "rimraf": "^2.6.2",
    "supertest": "~3.0.0",
    "ts-jest": "~22.4.4",
    "ts-node": "^6.0.2",
    "tsc": "~1.20150623.0",
    "tslint": "~5.9.1",
    "typedoc": "~0.11.1",
    "typedoc-plugin-external-module-name": "^1.1.1",
    "typedoc-plugin-internal-external": "^1.0.10",
    "typescript": "~2.8.3"
  }
}
