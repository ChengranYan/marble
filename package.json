{
  "name": "marblejs",
  "version": "0.1.1",
  "description": "Reactive Node APIs made easy",
  "scripts": {
    "postinstall": "npm run build",
    "build": "lerna bootstrap",
    "benchmark": "npm run build && make -C benchmarks",
    "example": "ts-node example/index.ts",
    "example:watch": "nodemon --watch packages --watch example --ext ts --ignore '*.spec.ts' --exec npm run example",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "test": "NODE_ENV=test jest --expand",
    "test:coverage": "NODE_ENV=test jest --expand --coverage",
    "test:watch": "NODE_ENV=test jest --expand --onlyChanged --watch",
    "prepublish": "npm run test",
    "publish": "lerna publish",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marblejs/marble.git"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "keywords": [
    "API",
    "JavaScript",
    "RxJs",
    "TypeScript",
    "Reactive"
  ],
  "authors": [
    "JÃ³zef Flakus"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marblejs/marble/issues"
  },
  "homepage": "https://github.com/marblejs/marble#readme",
  "dependencies": {
    "rxjs": "~6.1.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/supertest": "^2.0.4",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lerna": "^2.11.0",
    "mock-req": "^0.2.0",
    "nodemon": "^1.17.3",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "~22.4.4",
    "ts-node": "^6.0.2",
    "tsc": "~1.20150623.0",
    "tslint": "~5.9.1",
    "typedoc": "~0.11.1",
    "typedoc-plugin-external-module-name": "^1.1.1",
    "typedoc-plugin-internal-external": "^1.0.10",
    "typescript": "~2.8.3"
  }
}
